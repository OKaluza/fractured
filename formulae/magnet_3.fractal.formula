//Magnet3
// similar to kleinien group / mobius transform arangement:
// Mobius transorm z = az^2 + b / cz^2 + d

//z(n+1) =
@znext = expression((A*z^p+B) / (C*z^p-D) + c);
//Power (p)
@p = real(2);
@A = complex(1,0);
@B = complex(1,0);
@C = complex(1,0);
@D = complex(1,0);
//Bailout Test
@bailtest = bailout_function(norm);
//Escape
@escape = real(100);
//Converge
@converge = real(0.00005);

converged:
  //Converge test
  if (bailtest(z-(1,0)) < converge) break;
