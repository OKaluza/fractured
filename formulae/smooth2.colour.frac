//Smooth colouring algorithm v2
real ilp, lp;
void smooth2_init()
{
  //Smooth colouring algorithm
  ilp = 1.0/log(2.0);        //Inverse log of (power).
  //Additional for Smooth 2
  lp = log(2.0 * log(sqrt(bailout))) * ilp;
  ////ln(2 * ln bailout) / ln(2).
}
void smooth2_reset() {} 
void smooth2_calc() {}
real smooth2_result()
{
  //Smooth2 colouring
  return real(count) + lp - log(log(cabs(z))) * ilp;
}

