//z(n+1) =
@znext = expression((z^p + (c-a)*z - c) / (z^(p-1) + c - b));
//Power (p)
@p = real(2);
@a = complex(1,0);
@b = complex(1,0);
//Bailout Test
@bailtest = bailout_function(norm);
//Escape
@escape = real(100);
//Converge
@converge = real(0.00005);

converged:
  //Converge test
  if (bailtest(z-(1,0)) < converge) break;
