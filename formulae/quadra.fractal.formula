//z(n+1) =
@znext = expression((z^p + (c-a)*z - c) / (z^(p-1) + c - b));
//Power (p)
@p = real(2);
@a = complex(1,0);
@b = complex(1,0);
//Escape
@escape = real(100);
//Converge
@converge = real(0.00005);
//Bailout Function
@bailtest = bailout_function(norm);

//Convergent bailout
converged:
{
  return bailtest(z-(1,0)) < converge;
}