//Derived from Gumowski-Mira eqn f(x) = ax + 2(1-a)x^2 / (1 + x)
//z(n+1) =
@znext = expression(A*z + (C*(D-B)*z^p) / (D+z^p) + c);
//Power (p)
@p = real(2);
@A = complex(1,0);
@B = complex(1,0);
@C = complex(1,0);
@D = complex(1,0);
//Bailout Test
@bailtest = bailout_function(norm);
//Escape
@escape = real(100);
//Converge
@converge = real(0.00005);

converged:
  //Converge test
  converged = bailtest(z-(1,0)) < converge;
