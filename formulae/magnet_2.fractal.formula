//Magnet 2: z = ((z^3 + 3z(c-1) + (c-1)(c-2)) / (3*z^2 + 3z(c-2) + (c-1)(c-2)))^a
//z(n+1) = 
@znext = expression(((z^3 + 3*zc1 + c1c2) / (3*z^2 + 3*zc2 + c1c2 + 1))^p);
//c1c2 =
@c1c2 = expression((c-1).(c-2));
//zc1 =
@zc1 = expression(z.(c-1));
//zc2 =
@zc2 = expression(z.(c-2));
//Power (p)
@p = real(2);
//Bailout Test
@bailtest = bailout_function(norm);
//Escape
@escape = real(100);
//Converge
@converge = real(0.00005);

//Converge test
converged:
{
  return bailtest(z-(1,0)) < converge;
}