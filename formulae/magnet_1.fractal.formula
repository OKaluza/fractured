//Magnet: z = ((z^b + c - 1) / (b*z + c - 2))^a
//z(n+1) = 
@znext = expression(((z^q+c-1) / (q*z+c-2))^p);
//Power (p)
@p = real(2);
//Power (q)
@q = real(2);
//Escape
@escape = real(100);
//Converge
@converge = real(0.00005);
//Bailout Function
@bailtest = bailout_function(norm);

//Convergent bailout
converged:
{
  return bailtest(z-(1,0)) < converge;
}