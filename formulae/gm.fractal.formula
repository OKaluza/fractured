//Derived from Gumowski-Mira eqn f(x) = ax + 2(1-a)x^2 / (1 + x)
//z(n+1) =
@znext = expression(a*z + (2*(1-a)*z^p) / (1+z) + c);
//Power (p)
@p = real(2);
@a = complex(1,0);
//Bailout Test
@bailtest = bailout_function(norm);
//Escape
@escape = real(100);
//Converge
@converge = real(0.00005);

converged:
  //Converge test
  if (bailtest(z-(1,0)) < converge) break;
